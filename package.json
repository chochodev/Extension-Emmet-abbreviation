{
  "name": "custom-snippets",
  "displayName": "custom-snippets",
  "description": "An extension to add custom snippets",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "commands": [
      {
        "command": "customSnippets.addSnippet",
        "title": "Add Custom Snippet"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "activationEvents": [
    "onCommand:customSnippets.addSnippet"
  ],
  "main": "./src/extension.js"
}
